@model HelpDesk.Models.Utilisateur

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
 
    <link href="/assets/css/bootstrap.css" rel="stylesheet" />
    
    <!-- FONTAWESOME STYLE CSS -->
    <link href="/assets/css/font-awesome.min.css" rel="stylesheet" />
    <!-- CUSTOM STYLE CSS -->
    <link href="/assets/css/style.css" rel="stylesheet" />
    <!-- GOOGLE FONT -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css' />
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.3.0/respond.min.js"></script>
    <![endif]-->

    <title>Login2</title>

</head>
<body>

    <header>
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand" href="htttp://www.binarytheme.com">BNA HelpDesk</a>
                </div>
                <!-- Collect the nav links for toggling -->
                <div class="collapse navbar-collapse navbar-ex1-collapse">
                 
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container -->
        </nav>
    </header>
    <!--End Navigation -->
    <!--Header section  -->
    <div id="home">
        <div class="container">
            <div class="row ">
                <div class="col-md-9 col-sm-9">
                    <h1 class="head-main">HelpDesk</h1>
                    <span class="head-sub-main">La Banque  National D'Algérie</span>
            

                </div>
                <div class="col-md-5 col-sm-3">
                    <div class="div-trans text-center">
                        <h3>BIENVENNUE</h3>
                        @using (Html.BeginForm("Login", "Utilisateur", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
                        {
                            @Html.AntiForgeryToken()
                            <hr />
                            @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                            <div class="col-md-12 col-sm-12">
                                <div class="form-group">
                                    @Html.TextBoxFor(m => m.Email, new { @class = "form-control", @placeholder = "E-Mail", @type = "email" })
                                    @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })
                                    
                                </div>
                            </div>
                            <div class="col-md-12 col-sm-12">
                                <div class="form-group">
                                    @Html.PasswordFor(m => m.Password, new { @class = "form-control", @placeholder = "Mot de Passe" })
                                    @Html.ValidationMessageFor(m => m.Password, "", new { @class = "text-danger" })
                                </div>
                            </div>


                            <div class="col-md-12 col-sm-12">
                               
                                <div class="form-group">
                                    
                                        <input type="submit" value="Log in" class="btn btn-default" />
                                   
                                </div>
                                <p>
                                    @Html.ActionLink("Forgot your password?", "Forgot", "Utilisateur")


                                </p>
                            </div>

                        }
                    </div>

                </div>

            </div>
        </div>
    </div>
    <!--End Header section  -->
    <!--Services Section-->
    <section id="services">
        <div class="container">

            <div class="row text-center">
                <div class="col-md-8 col-md-offset-2">
                    <h2>Our Services</h2>

                </div>

            </div>

            <div class="row text-center space-pad">
                <div class="col-md-3 col-sm-3">

                    <div>

                        <i class="fa fa-desktop fa-4x main-color"></i>


                        <h3>DDEP </h3>
                        <p>
                            Direction du Développement Etudes et Projets :
                           

                            	Commerce Extérieur.
                            	Engagement
                            	Crédit.
                            	Caution.
                            	Front.

                        </p>

                    </div>

                </div>
                <div class="col-md-3 col-sm-3">

                    <div>

                        <i class="fa fa-desktop fa-4x main-color"></i>


                        <h3>DPRH </h3>
                        <p>
                            Lorem ipsum dolor sit amet, consectetur adipiscing elit.
                            Curabitur nec nisl odio. Mauris vehicula at nunc id posuere.
                        </p>

                    </div>

                </div>
                <div class="col-md-3 col-sm-3">

                    <div>

                        <i class="fa fa-desktop fa-4x main-color"></i>


                        <h3>DTA </h3>
                        <p>
                            Direction des Technologies et de l’Architecture  :
                           
                            	Technologie 
                        
                           	Session
                            	
                            	 Sécurité 
                           
                          
                            C-	  Réseau:
                        

                        </p>

                    </div>

                </div>
                <div class="col-md-3 col-sm-3">

                    <div>

                        <i class="fa fa-desktop fa-4x main-color"></i>


                        <h3>DPRS </h3>
                        <p>
                           	Les Modules de Direction de la Production et des Services  :
                            
                            	CAPTURE.
                            	PRESENTATION.
                            	GIP.

                        </p>

                    </div>

                </div>
            </div>
         
        </div>


    </section>
    <!--End Services Section-->
    <!--parallax one-->
    <section id="Parallax-one">
        <div class="container">

         

        </div>
    </section>
   
    <!--./parallax two-->
    <!-- Contact Section -->
    <section id="contact-sec">
        <div class="container">
            <div class="row">
                <div class="col-md-12">

                    <div id="social-icon">
                        <strong> Address:</strong> 124/56 , Baba Hassan, Alger.
                       
                    </div>
                </div>


            </div>
        </div>
    </section>

    <!--End Contact Section -->
    <!--footer Section -->
    <div class="for-full-back " id="footer">
       @DateTime.Now www.BNA.com | All Right Reserved

    </div>
    <!--End footer Section -->
    <!-- JAVASCRIPT FILES PLACED AT THE BOTTOM TO REDUCE THE LOADING TIME  -->
    <!-- CORE JQUERY  -->
    <script src="~/~/Views/Utilisateur/assets/plugins/jquery-1.10.2.js"></script>
    <!-- BOOTSTRAP CORE SCRIPT   -->
    <script src="~/~/Views/Utilisateur/assets/plugins/bootstrap.js"></script>
    <!-- PARALLAX SCRIPT   -->
    <script src="~/~/Views/Utilisateur/assets/plugins/4jquery.parallax-1.1.3.js"></script>
    <!-- CUSTOM SCRIPT   -->
    <script src="~/~/Views/Utilisateur/assets/js/custom.js"></script>
    
   
</body>
</html>
