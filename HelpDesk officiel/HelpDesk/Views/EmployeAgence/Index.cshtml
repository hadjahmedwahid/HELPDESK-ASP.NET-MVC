@model HelpDesk.Models.EmployeAgence

@{
	ViewBag.Title = "Index";
}
		<div class="panel-body widget-shadow">
			<h2>Index</h2>

			<p>
				@Html.ActionLink("Create New", "Create")
			</p>
			<table class="table">
				<tr>
					<th>
						@Html.DisplayNameFor(model => model.Nom)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Prenom)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Login)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Password)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Email)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.Phone)
					</th>
					<th>
						@Html.DisplayNameFor(model => model.EtatUser)
					</th>

					<th>
						@Html.DisplayNameFor(model => model.agence)
					</th>
					<th></th>
				</tr>

				@foreach (var item in Model.find_all())
				{
					<tr>
						<td>
							@Html.DisplayFor(modelItem => item.Nom)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.Prenom)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.Login)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.Password)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.Email)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.Phone)
						</td>
						<td>
							@Html.DisplayFor(modelItem => item.EtatUser)
						</td>

						<td>
							@Html.DisplayFor(modelItem => item.agence.NomAgence)
						</td>
						<td>
							@Html.ActionLink("Edit", "Edit", new { id = item.IdUser }) |
							@Html.ActionLink("Details", "Details", new { id = item.IdUser }) |
							@Html.ActionLink("Delete", "Delete", new { id = item.IdUser })
						</td>
					</tr>
				}

			</table>
		</div>